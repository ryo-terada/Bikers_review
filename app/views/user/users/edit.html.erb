<h2>プロフィール編集</h2>
<%= form_with model: @user do |f| %>
  <label for="inputName">Name</label>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前" %>
  <label for="inputIntroduction">Introduction</label>
  <%= f.text_area :introduction, id:"inputIntroduction" , placeholder:"自己紹介文" %>
  <label for="inputImage">UserImage</label>
  <%= f.file_field :user_image, placeholder: "プロフィール画像", accept: "image/*" %>  
  <%= f.submit "変更を保存" %>
<% end %>